<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ece251.gongxl.transcanner"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ece251.gongxl.transcanner.Welcome"
            android:label="@string/title_activity_welcome"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ece251.gongxl.transcanner.MainMenu"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.ece251.gongxl.transcanner.Camera"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name="com.ece251.gongxl.transcanner.Translation"
            android:label="@string/title_activity_translation" >
        </activity>
        <activity
            android:name="com.ece251.gongxl.transcanner.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name="com.ece251.gongxl.transcanner.ScanResult"
            android:label="@string/title_activity_scan_result" >
        </activity>
    </application>

</manifest>